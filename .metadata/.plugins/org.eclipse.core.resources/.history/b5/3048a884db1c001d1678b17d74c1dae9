public class CodeGenerator extends VisitorAdaptor {
	
	private int varCount;
	
	private int paramCnt;
	
	private int mainPc;
	
	public int getMainPc() {
		return mainPc;
	}
	
	private int opcoder(String instruction) {
		switch (instruction) {
		case "load":
			return 1;
		case "load_0":
			return 2;
		case "load_1":
			return 3;
		case "load_2":
			return 4;
		case "load_3":
			return 5;
		case "store":
			return 6;
		case "store_0":
			return 7;
		case "store_1":
			return 8;
		case "store_2":
			return 9;
		case "store_3":
			return 10;
		}
		
	}
}
